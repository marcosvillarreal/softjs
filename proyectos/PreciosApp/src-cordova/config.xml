<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cordova.preciosapp.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Santarossa Precios</name>
    <description>PreciosApp</description>
    <icon src="res/icon-36-ldpi.png" />
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.0.1">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-gmv-barcode-scanner" spec="^1.2.2" />
    <allow-navigation href="http://192.168.2.103:8080/" />
    <plugin name="cordova-plugin-speechrecognition" spec="^1.2.0" />
    <plugin name="cordova-plugin-media-capture" spec="^3.0.3" />
    <allow-navigation href="about:*" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />
    <engine name="android" spec="^7.1.4" />
</widget>
